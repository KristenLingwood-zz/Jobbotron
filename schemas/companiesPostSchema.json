{
  "$id": "http://example.com/example.json",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "name": {
      "$id": "/properties/name",
      "type": "string",
      "title": "The Name Schema ",
      "default": "",
      "examples": ["Nakatomi Trading Corp."],
      "minLength": 1,
      "maxLength": 55
    },
    "email": {
      "$id": "/properties/email",
      "type": "string",
      "format": "email",
      "title": "The Email Schema ",
      "default": "",
      "examples": ["smd@email.com"],
      "minLength": 6,
      "maxLength": 60
    },
    "handle": {
      "$id": "/properties/handle",
      "type": "string",
      "title": "The Handle Schema ",
      "default": "",
      "examples": ["nakatomi"],
      "minLength": 1,
      "maxLength": 55
    },
    "logo": {
      "$id": "/properties/logo",
      "type": "string",
      "format": "uri",
      "title": "The Logo Schema ",
      "default": "",
      "examples": [
        "https://ih1.redbubble.net/image.506058298.5768/flat,550x550,075,f.u8.jpg"
      ],
      "minLength": 1
    },
    "password": {
      "$id": "/properties/password",
      "type": "string",
      "title": "The Password Schema ",
      "default": "",
      "examples": ["lastchristmas"],
      "minLength": 1,
      "maxLength": 55
    },
    "employees:": {
      "$id": "/properties/employees",
      "type": "array",
      "title": "The employees Schema ",
      "default": [],
      "examples": []
    },
    "jobs:": {
      "$id": "/properties/jobs",
      "type": "array",
      "title": "The jobs Schema ",
      "default": [],
      "examples": []
    }
  },
  "required": ["name", "email", "handle", "password"]
}
