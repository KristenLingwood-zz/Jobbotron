{
  "$id": "http://example.com/example.json",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "first_name": {
      "$id": "/properties/first_name",
      "type": "string",
      "title": "The First_name Schema ",
      "default": "",
      "examples": ["Sleve"],
      "minLength": 1,
      "maxLength": 55
    },
    "last_name": {
      "$id": "/properties/last_name",
      "type": "string",
      "title": "The Last_name Schema ",
      "default": "",
      "examples": ["McDichael"],
      "minLength": 1,
      "maxLength": 55
    },
    "email": {
      "$id": "/properties/email",
      "type": "string",
      "format": "email",
      "title": "The Email Schema ",
      "default": "",
      "examples": ["smd@email.com"],
      "minLength": 6,
      "maxLength": 60
    },
    "photo": {
      "$id": "/properties/photo",
      "type": "string",
      "format": "uri",
      "title": "The Photo Schema ",
      "default": "",
      "examples": [
        "https://image.shutterstock.com/image-photo/happy-businessman-isolated-handsome-man-260nw-609414131.jpg"
      ],
      "minLength": 1
    },
    "username": {
      "$id": "/properties/username",
      "type": "string",
      "title": "The Username Schema ",
      "default": "",
      "examples": ["slevey_mcd"],
      "minLength": 1,
      "maxLength": 55
    },
    "password": {
      "$id": "/properties/password",
      "type": "string",
      "title": "The Password Schema ",
      "default": "",
      "examples": ["mcd"],
      "minLength": 5,
      "maxLength": 55
    },
    "current_company": {
      "$id": "/properties/current_company",
      "type": "string",
      "title": "The current_company Schema ",
      "default": "",
      "examples": ["RithmSkool"]
    },
    "applied_to:": {
      "$id": "/properties/applied_to",
      "type": "array",
      "title": "The applied_to Schema ",
      "default": [],
      "examples": []
    }
  },
  "required": ["first_name", "last_name", "email", "username", "password"]
}
